<routes>
    <vType id="car"
   			vClass="passenger" length="5" accel="3.5" decel="2.2"
   			sigma="1.0" maxSpeed="10" />

    <vehicles>
        <flow id="Car-Route-Left2Top" begin="0" end="1000" from="left-approaching" 
        to="top-exit" probability="0.5" type="car"/>
        <flow id="Car-Route-Left2Right" begin="0" end="1000" from="left-approaching" 
        to="right-exit" probability="0.5" type="car"/>
        <flow id="Car-Route-Left2Bottom" begin="0" end="1000" from="left-approaching" 
        to="bottom-exit" probability="0.5" type="car"/>

        <flow id="Car-Route-Top2Right" begin="0" end="1000" from="top-approaching" 
        to="right-exit" probability="0.5" type="car"/>
        <flow id="Car-Route-Top2Bottom" begin="0" end="1000" from="top-approaching" 
        to="bottom-exit" probability="0.5" type="car"/>
        <flow id="Car-Route-Top2Left" begin="0" end="1000" from="top-approaching" 
        to="left-exit" probability="0.5" type="car"/>

        <flow id="Car-Route-Right2Bottom" begin="0" end="1000" from="right-approaching" 
        to="bottom-exit" probability="0.5" type="car"/>
        <flow id="Car-Route-Right2Left" begin="0" end="1000" from="right-approaching" 
        to="left-exit" probability="0.5" type="car"/>
        <flow id="Car-Route-Right2Top" begin="0" end="1000" from="right-approaching" 
        to="top-exit" probability="0.5" type="car"/>

        <flow id="Car-Route-Bottom2Left" begin="0" end="1000" from="bottom-approaching" 
        to="bottom-exit" probability="0.5" type="car"/>
        <flow id="Car-Route-Bottom2Top" begin="0" end="1000" from="bottom-approaching" 
        to="top-exit" probability="0.5" type="car"/>
        <flow id="Car-Route-Bottom2Right" begin="0" end="1000" from="bottom-approaching" 
        to="right-exit" probability="0.5" type="car"/>        
    </vehicles>
</routes>

