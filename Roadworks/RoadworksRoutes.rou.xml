<routes>
    <vType id="car"
   			vClass="passenger" length="5" accel="2.5" decel="2.2"
   			sigma="1.0" maxSpeed="10" color="green"/>

    <vType id="stoppedVehicle"
   			vClass="truck" length="10" accel="2.5" decel="2.2"
   			sigma="1.0" maxSpeed="10" color="blue"/>

    <vehicles>
        <flow id="Car-Route-Left2Top" begin="0" end="100"  type="car" number="2" departLane="random">
            <route edges="left-long-approaching left-short-approaching top-exit" color="green" />
        </flow>
        <flow id="Car-Route-Left2Right" begin="0" end="100" type="car" number="2" departLane="random">
            <route edges="left-long-approaching left-short-approaching right-exit" />
        </flow>
        <flow id="Car-Route-Left2Bottom" begin="0" end="100" type="car" number="2" departLane="random">
            <route edges="left-long-approaching left-short-approaching bottom-exit" />
        </flow>

        <flow id="Car-Route-Top2Right" begin="0" end="100" type="car" number="5" departLane="random">
            <route edges="top-approaching right-exit" color="red"/>
        </flow>
        <flow id="Car-Route-Top2Bottom" begin="0" end="100" type="car" number="5" departLane="random">
            <route edges="top-approaching bottom-exit" color="yellow" />
        </flow>
        <flow id="Car-Route-Top2Left" begin="0" end="100" type="car" number="5" departLane="random">
            <route edges="top-approaching closed-lane remerging-lane left-exit" color="blue"  />
        </flow>

        <flow id="Car-Route-Right2Bottom" begin="0" end="1000" type="car" number="2" departLane="random">
            <route edges="right-approaching bottom-exit" />
        </flow>
        <flow id="Car-Route-Right2Left" begin="0" end="100" type="car" number="2" departLane="random">
            <route edges="right-approaching closed-lane remerging-lane left-exit" />
        </flow>
        <flow id="Car-Route-Right2Top" begin="0" end="1000" type="car" number="2" departLane="random">
            <route edges="right-approaching top-exit" />
        </flow>

        
        <flow id="Car-Route-Bottom2Left" begin="30" end="100" type="car" number="5" departLane="random">
            <route edges="bottom-approaching closed-lane remerging-lane left-exit" />
        </flow>
        <flow id="Car-Route-Bottom2Top" begin="30" end="100"  type="car" number="5" departLane="random">
            <route edges="bottom-approaching top-exit" />
        </flow>
        <flow id="Car-Route-Bottom2Right" begin="30" end="100" type="car" number="5" departLane="random">
            <route edges="bottom-approaching right-exit" />
        </flow>  
        
        
       

    </vehicles>
</routes>

